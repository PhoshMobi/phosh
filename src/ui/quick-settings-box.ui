<?xml version="1.0" encoding="utf-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
  <template class="PhoshQuickSettingsBox" parent="GtkContainer"/>
  <object class="GtkRevealer" id="revealer">
    <property name="visible">1</property>
    <signal name="notify::child-revealed" handler="on_child_revealed_changed" object="PhoshQuickSettingsBox" swapped="yes"/>
  </object>
</interface>
